    <script>
        $(document).ready(function () {
            $.ajax({
                method: 'get',
                url: `/api/chapter/next-chapter/<%=chapter._id%>`
            }).done((data) => {
                if (data.prevChapter) {
                    $('#prev_chap').removeClass("disabled").attr('href', `/${data.manga.slug}/${data.prevChapter.slug}`);
                    $('#prev_chap_bot').removeClass("disabled").attr('href', `/${data.manga.slug}/${data.prevChapter.slug}`)
                }
                if (data.nextChapter) {
                    $('#next_chap').removeClass("disabled").attr('href', `/${data.manga.slug}/${data.nextChapter.slug}`);
                    $('#next_chap_bot').removeClass("disabled").attr('href', `/${data.manga.slug}/${data.nextChapter.slug}`);
                }
            })
            $('.chapter_jump_list').unbind('click');
            $('.chapter_jump_list').click(function () {
                $.ajax({
                    method: 'get',
                    url: `/api/chapter/list-chapter/<%=manga._id%>`
                })
                    .done(data => {
                        const chapterId = '<%=chapter._id%>';
                        const listOption = data.map(chapter => {
                            const selected = chapterId==chapter._id ? 'selected': null;
                            return `<option value="/<%=manga.slug%>/${chapter.slug}" ${selected}>${chapter.title}</option>`
                        })
                        const html = `
                        <select class="btn btn-success btn-chapter-nav form-control chapter_jump_select">
                            ${listOption.join('')}
                        </select>
                        `;
                        $('.chapter_jump_list').remove();
                        $('#prev_chap').after(html);
                        $('#prev_chap_bot').after(html);
                        $(".chapter_jump_select").change(function(){
                            console.log(this.value);
                            window.location.replace(this.value);
                        })
                    })
            })

        })
    </script>
